# NODUS Relay Server Environment Variables
# Copy to .env and fill with secure random values

# Required: 64-character random strings
TURN_SECRET=generate_random_64_chars_here_using_openssl_rand_hex_32
CRED_SECRET=generate_random_64_chars_here_using_openssl_rand_hex_32

# Optional: Database and cache
DATABASE_URL=postgresql://user:pass@host:5432/nodus
REDIS_URL=redis://localhost:6379

# Server config
PORT=3000
NODE_ENV=production
