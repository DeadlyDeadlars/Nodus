{
  "name": "NODUS",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode=release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --mode=Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:crypto": "jest --testPathPattern=crypto",
    "test:security": "jest --testPathPattern=security",
    "typecheck": "tsc --noEmit",
    "clean": "cd android && ./gradlew clean && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "install:release": "cd android && ./gradlew installRelease"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "base-64": "^1.0.0",
    "qrcode": "^1.5.4",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-argon2": "^4.0.0",
    "react-native-audio-recorder-player": "^3.6.0",
    "react-native-document-picker": "^8.1.3",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-incall-manager": "^4.0.1",
    "react-native-keychain": "^10.0.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-qrcode-skia": "^0.3.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-ssl-pinning": "^1.6.0",
    "react-native-svg": "^13.14.0",
    "react-native-video": "^6.18.0",
    "react-native-vision-camera": "^4.7.3",
    "react-native-webrtc": "^124.0.5",
    "text-encoding": "^0.7.0",
    "tweetnacl": "^1.0.3",
    "zustand": "^5.0.9"
